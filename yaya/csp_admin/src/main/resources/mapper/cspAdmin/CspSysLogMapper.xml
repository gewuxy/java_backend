<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="cn.medcn.csp.admin.dao.CspSysLogDAO" >

    <select id="findCspSysLog" resultType="cn.medcn.csp.admin.model.CspSysLog">
        SELECT * FROM t_csp_log WHERE 1=1
        <if test="account != null">
            AND account = #{account}
        </if>
        order by log_date desc
    </select>

</mapper>